{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " \uf0e7 ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#ffe9aa",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "folder_icon": "üìÅ",
            "folder_separator_icon": "/",
            "style": "full"
          },
          "template": " {{ .Path }} ",
          "foreground": "#100e23",
          "powerline_symbol": "\ue0b0",
          "background": "#88c0d0",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "foreground": "#193549",
          "powerline_symbol": "\ue0b0",
          "background": "#a3b38c",
          "type": "git",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " üö´ {{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}\uf42e{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#ff8080",
          "type": "status",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": " üïë {{ .FormattedMs }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#88c0d0",
          "type": "executiontime",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "‚ùØ ",
          "foreground": "#007ACC",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
